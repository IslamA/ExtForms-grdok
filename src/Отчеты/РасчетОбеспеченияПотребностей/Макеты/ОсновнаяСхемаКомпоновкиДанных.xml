				<dataSource>ИсточникДанных1</dataSource>
				<query>ВЫБРАТЬ
	Рег.ЗаказКлиента КАК Документ,
	Рег.Номенклатура КАК Номенклатура,
	Рег.Характеристика КАК Характеристика,
	Рег.КодСтроки КАК КодСтроки,
	Рег.Склад КАК Склад,
	Рег.Серия КАК Серия,
	Рег.ЗаказаноОстаток КАК Заказано,
	ДокТовары.ВариантОбеспечения КАК ВариантОбеспечения,
	ДокТовары.Ссылка.ДатаОтгрузки КАК ДатаОтгрузки,
	ВЫБОР
		КОГДА ДокТовары.ВариантОбеспечения В (ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.Требуется), ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.Обособленно))
			ТОГДА Рег.ЗаказаноОстаток
		ИНАЧЕ 0
	КОНЕЦ КАК КОбеспечению,
	ВЫБОР
		КОГДА ДокТовары.ВариантОбеспечения В (ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.ИзЗаказов))
			ТОГДА Рег.ЗаказаноОстаток
		ИНАЧЕ 0
	КОНЕЦ КАК РезервироватьКДате,
	ВЫБОР
		КОГДА ДокТовары.ВариантОбеспечения В (ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.СоСклада))
			ТОГДА Рег.ЗаказаноОстаток
		ИНАЧЕ 0
	КОНЕЦ КАК РезервироватьНаСкладе,
	ВЫБОР
		КОГДА ДокТовары.ВариантОбеспечения В (ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.ИзЗаказов))
			ТОГДА -Рег.ЗаказаноОстаток
		ИНАЧЕ 0
	КОНЕЦ КАК КоличествоПроизводство,
	ВЫБОР
		КОГДА ДокТовары.ВариантОбеспечения В (ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.Отгрузить), ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.ОтгрузитьОбособленно))
			ТОГДА Рег.ЗаказаноОстаток
		ИНАЧЕ 0
	КОНЕЦ КАК Отгрузить,
	ВЫБОР
		КОГДА ДокТовары.ВариантОбеспечения В (ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.Отгрузить), ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.ОтгрузитьОбособленно), ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.СоСклада))
			ТОГДА -Рег.ЗаказаноОстаток
		ИНАЧЕ 0
	КОНЕЦ КАК ВНаличии,
	Рег.ЗаказКлиента.Назначение КАК Назначение
{ВЫБРАТЬ
	Документ.*,
	Номенклатура.*,
	Характеристика.*,
	Назначение.*,
	КодСтроки,
	Склад.*,
	Серия.*,
	ВариантОбеспечения.*,
	ДатаОтгрузки,
	Заказано,
	КОбеспечению,
	РезервироватьКДате,
	РезервироватьНаСкладе,
	Отгрузить,
	КоличествоПроизводство,
	ВНаличии}
ИЗ
	РегистрНакопления.ЗаказыКлиентов.Остатки(, {(ЗаказКлиента).* КАК Документ, (Номенклатура).*, (Характеристика).*, (Склад).*, (Серия).*, (КодСтроки), (ЗаказКлиента.Назначение).* КАК Назначение}) КАК Рег
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЗаказКлиента.Товары КАК ДокТовары
		ПО Рег.ЗаказКлиента = ДокТовары.Ссылка
			И Рег.КодСтроки = ДокТовары.КодСтроки
{ГДЕ
	ДокТовары.ВариантОбеспечения.* КАК ВариантОбеспечения,
	ДокТовары.ДатаОтгрузки}</query>
