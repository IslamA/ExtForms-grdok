				<query>ВЫБРАТЬ
	Рег2.ГоловнаяОрганизация КАК Организация,
	Рег1.Сотрудник КАК Сотрудник,
	Рег2.ВкладВФОТ КАК ФОТ,
	Рег2.Начисление КАК ВидРасчета,
	Рег2.РегистраторСобытия КАК Документ
{ВЫБРАТЬ
	Организация.*,
	Сотрудник.*,
	ФОТ,
	ВидРасчета.*,
	Документ.*}
ИЗ
	(ВЫБРАТЬ
		Рег.Сотрудник КАК Сотрудник,
		МАКСИМУМ(Рег.ДатаОкончания) КАК ДатаОкончания
	ИЗ
		РегистрСведений.ПлановыйФОТ КАК Рег
	ГДЕ
		Рег.ДатаОкончания &gt;= &amp;Дата2
		И Рег.Сотрудник В
				(ВЫБРАТЬ
					РегистрРасчета.Начисления.Сотрудник
				ИЗ
					РегистрРасчета.Начисления
				ГДЕ
					РегистрРасчета.Начисления.Месяц МЕЖДУ &amp;Дата1 И &amp;Дата2)
	{ГДЕ
		Рег.Сотрудник.*,
		Рег.ГоловнаяОрганизация.* КАК Организация,
		Рег.Начисление.* КАК ВидРасчета}
	
	СГРУППИРОВАТЬ ПО
		Рег.Сотрудник) КАК Рег1
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.ПлановыйФОТ КАК Рег2
		ПО Рег1.Сотрудник = Рег2.Сотрудник
			И Рег1.ДатаОкончания = Рег2.ДатаОкончания
ГДЕ
	Рег2.Используется
{ГДЕ
	Рег2.ВкладВФОТ КАК ФОТ}</query>
