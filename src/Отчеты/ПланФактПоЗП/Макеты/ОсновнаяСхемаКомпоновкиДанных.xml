				<dataSource>ИсточникДанных1</dataSource>
				<query>ВЫБРАТЬ
	Начисления.Организация КАК Организация,
	ЕСТЬNULL(КадроваяИстория.ДолжностьПоШтатномуРасписанию.Подразделение, ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)) КАК Подразделение,
	КадроваяИстория.ДолжностьПоШтатномуРасписанию КАК ДолжностьПоШтатномуРасписанию,
	Начисления.Сотрудник КАК Сотрудник,
	Начисления.Сотрудник.ФизическоеЛицо КАК ФизическоеЛицо,
	Начисления.НачислениеУдержание КАК ВидРасчета,
	Начисления.Регистратор КАК Документ,
	Начисления.Сумма КАК Факт,
	0 КАК Количество
{ВЫБРАТЬ
	Сотрудник.*,
	ФизическоеЛицо.*,
	Факт,
	ВидРасчета.*,
	Организация.*,
	Подразделение.*,
	Факт,
	Количество,
	Документ.*,
	ДолжностьПоШтатномуРасписанию.*}
ИЗ
	РегистрНакопления.НачисленияУдержанияПоСотрудникам КАК Начисления
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.КадроваяИсторияСотрудников.СрезПоследних(&amp;Дата2, {(ГоловнаяОрганизация).* КАК Организация, (Сотрудник).*}) КАК КадроваяИстория
		ПО Начисления.Сотрудник = КадроваяИстория.Сотрудник
ГДЕ
	Начисления.Период МЕЖДУ &amp;Дата1 И &amp;Дата2
	И Начисления.ГруппаНачисленияУдержанияВыплаты = ЗНАЧЕНИЕ(Перечисление.ГруппыНачисленияУдержанияВыплаты.Начислено)
{ГДЕ
	Начисления.Сотрудник.*,
	Начисления.Организация.* КАК Организация,
	Начисления.НачислениеУдержание.* КАК ВидРасчета,
	Начисления.Сумма КАК Факт,
	КадроваяИстория.ДолжностьПоШтатномуРасписанию.*,
	(ЕСТЬNULL(КадроваяИстория.ДолжностьПоШтатномуРасписанию.Подразделение, ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка))).* КАК Подразделение}</query>
