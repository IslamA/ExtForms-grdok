			<dataSource>ИсточникДанных1</dataSource>
			<query>ВЫБРАТЬ
	ЗаказыКлиентовОстатки.ЗаказКлиента,
	ЗаказыКлиентовОстатки.Номенклатура,
	ЗаказыКлиентовОстатки.Характеристика,
	ЗаказыКлиентовОстатки.Склад,
	ЗаказыКлиентовОстатки.ЗаказаноОстаток,
	ЗаказыКлиентовОстатки.КодСтроки,
	ЗаказыКлиентовОстатки.СуммаОстаток
ПОМЕСТИТЬ ВТ_ОстаткиЗаказов
ИЗ
	РегистрНакопления.ЗаказыКлиентов.Остатки(&amp;ДатаКонтроля, ) КАК ЗаказыКлиентовОстатки
ГДЕ
	ЗаказыКлиентовОстатки.ЗаказаноОстаток &gt; 0
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗаказКлиентаТовары.ВариантОбеспечения,
	ЗаказКлиентаТовары.Ссылка,
	ЗаказКлиентаТовары.Номенклатура,
	ЗаказКлиентаТовары.Характеристика,
	ЗаказКлиентаТовары.Склад,
	ЗаказКлиентаТовары.КодСтроки,
	ЗаказКлиентаТовары.ДатаОтгрузки
ПОМЕСТИТЬ ВТ_Заказы
ИЗ
	Документ.ЗаказКлиента.Товары КАК ЗаказКлиентаТовары
ГДЕ
	ЗаказКлиентаТовары.Отменено = ЛОЖЬ

СГРУППИРОВАТЬ ПО
	ЗаказКлиентаТовары.Склад,
	ЗаказКлиентаТовары.Номенклатура,
	ЗаказКлиентаТовары.Ссылка,
	ЗаказКлиентаТовары.ВариантОбеспечения,
	ЗаказКлиентаТовары.Характеристика,
	ЗаказКлиентаТовары.КодСтроки,
	ЗаказКлиентаТовары.ДатаОтгрузки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ОстаткиЗаказов.Номенклатура,
	ВТ_ОстаткиЗаказов.Характеристика,
	ВТ_ОстаткиЗаказов.Склад,
	СУММА(ВТ_ОстаткиЗаказов.ЗаказаноОстаток) КАК Заказано,
	СУММА(ВТ_ОстаткиЗаказов.СуммаОстаток) КАК ЗаказаноСумма,
	СУММА(ВЫБОР
			КОГДА ВТ_Заказы.ВариантОбеспечения = ЗНАЧЕНИЕ(перечисление.Вариантыобеспечения.СоСклада)
				ТОГДА ВТ_ОстаткиЗаказов.ЗаказаноОстаток
			ИНАЧЕ 0
		КОНЕЦ) КАК ОтгрузитьРезервировать,
	СУММА(ВЫБОР
			КОГДА ВТ_Заказы.ВариантОбеспечения = ЗНАЧЕНИЕ(перечисление.Вариантыобеспечения.ИзЗаказов)
				ТОГДА ВТ_ОстаткиЗаказов.ЗаказаноОстаток
			ИНАЧЕ 0
		КОНЕЦ) КАК РезервироватьКДатеОтгрузки,
	ВТ_ОстаткиЗаказов.ЗаказКлиента,
	ВТ_Заказы.ДатаОтгрузки,
	СУММА(ВЫБОР
			КОГДА ВТ_Заказы.ВариантОбеспечения = ЗНАЧЕНИЕ(перечисление.Вариантыобеспечения.Обособленно)
				ТОГДА ВТ_ОстаткиЗаказов.ЗаказаноОстаток
			ИНАЧЕ 0
		КОНЕЦ) КАК Обособленно
ПОМЕСТИТЬ ВТ_ЗаказанныйТовар
ИЗ
	ВТ_ОстаткиЗаказов КАК ВТ_ОстаткиЗаказов
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_Заказы КАК ВТ_Заказы
		ПО ВТ_ОстаткиЗаказов.ЗаказКлиента = ВТ_Заказы.Ссылка
			И ВТ_ОстаткиЗаказов.Номенклатура = ВТ_Заказы.Номенклатура
			И ВТ_ОстаткиЗаказов.Характеристика = ВТ_Заказы.Характеристика
			И ВТ_ОстаткиЗаказов.Склад = ВТ_Заказы.Склад
			И ВТ_ОстаткиЗаказов.КодСтроки = ВТ_Заказы.КодСтроки

СГРУППИРОВАТЬ ПО
	ВТ_ОстаткиЗаказов.Номенклатура,
	ВТ_ОстаткиЗаказов.Склад,
	ВТ_ОстаткиЗаказов.Характеристика,
	ВТ_ОстаткиЗаказов.ЗаказКлиента,
	ВТ_Заказы.ДатаОтгрузки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СвободныеОстаткиОстатки.Номенклатура,
	СвободныеОстаткиОстатки.Характеристика,
	СвободныеОстаткиОстатки.Склад,
	СвободныеОстаткиОстатки.ВНаличииОстаток,
	СвободныеОстаткиОстатки.ВРезервеСоСкладаОстаток,
	СвободныеОстаткиОстатки.ВРезервеПодЗаказОстаток
ПОМЕСТИТЬ ВТ_СвободныеОстатки
ИЗ
	РегистрНакопления.СвободныеОстатки.Остатки(&amp;ДатаКонтроля, ) КАК СвободныеОстаткиОстатки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ЗаказанныйТовар.ЗаказКлиента,
	ВТ_ЗаказанныйТовар.ЗаказКлиента.Партнер КАК Партнер,
	ВТ_ЗаказанныйТовар.ЗаказКлиента.Организация КАК Организация,
	ВТ_ЗаказанныйТовар.ЗаказКлиента.Партнер.ОсновнойМенеджер КАК Менеджер,
	ВТ_ЗаказанныйТовар.Склад,
	ВТ_ЗаказанныйТовар.Номенклатура,
	ВТ_ЗаказанныйТовар.Характеристика,
	ВТ_ЗаказанныйТовар.ДатаОтгрузки,
	ВТ_ЗаказанныйТовар.Заказано,
	ВТ_ЗаказанныйТовар.ЗаказаноСумма,
	ВТ_ЗаказанныйТовар.ОтгрузитьРезервировать,
	ВТ_ЗаказанныйТовар.РезервироватьКДатеОтгрузки,
	РАЗНОСТЬДАТ(ВТ_ЗаказанныйТовар.ДатаОтгрузки, &amp;ДатаКонтроля, ДЕНЬ) КАК ПросроченоДнейОтгрузки,
	ВТ_СвободныеОстатки.ВНаличииОстаток,
	ВТ_СвободныеОстатки.ВРезервеСоСкладаОстаток,
	ВТ_СвободныеОстатки.ВРезервеПодЗаказОстаток,
	ВТ_ЗаказанныйТовар.Обособленно
ИЗ
	ВТ_ЗаказанныйТовар КАК ВТ_ЗаказанныйТовар
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СвободныеОстатки КАК ВТ_СвободныеОстатки
		ПО ВТ_ЗаказанныйТовар.Номенклатура = ВТ_СвободныеОстатки.Номенклатура
			И ВТ_ЗаказанныйТовар.Характеристика = ВТ_СвободныеОстатки.Характеристика
			И ВТ_ЗаказанныйТовар.Склад = ВТ_СвободныеОстатки.Склад</query>
		</dataSet>
		<calculatedField>
			<dataPath>ДоступноДляПродажи</dataPath>
			<expression>ВНаличииОстаток-ОтгрузитьРезервировать-РезервироватьКДатеОтгрузки</expression>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Доступно для продажи</d4p1:content>
				</d4p1:item>
			</title>
		</calculatedField>
		<totalField>
			<dataPath>ВНаличииОстаток</dataPath>
			<expression>Сумма(ВНаличииОстаток)</expression>
		</totalField>
		<totalField>
			<dataPath>ВРезервеПодЗаказОстаток</dataPath>
			<expression>Максимум(ВРезервеПодЗаказОстаток)</expression>
		</totalField>
		<totalField>
			<dataPath>ВРезервеСоСкладаОстаток</dataPath>
			<expression>Максимум(ВРезервеСоСкладаОстаток)</expression>
		</totalField>
		<totalField>
			<dataPath>Заказано</dataPath>
			<expression>Сумма(Заказано)</expression>
		</totalField>
		<totalField>
			<dataPath>ЗаказаноСумма</dataPath>
			<expression>Сумма(ЗаказаноСумма)</expression>
		</totalField>
		<totalField>
			<dataPath>ОтгрузитьРезервировать</dataPath>
			<expression>Сумма(ОтгрузитьРезервировать)</expression>
		</totalField>
		<totalField>
			<dataPath>ПросроченоДнейОтгрузки</dataPath>
			<expression>Сумма(ПросроченоДнейОтгрузки)</expression>
		</totalField>
		<totalField>
			<dataPath>РезервироватьКДатеОтгрузки</dataPath>
			<expression>Сумма(РезервироватьКДатеОтгрузки)</expression>
		</totalField>
		<totalField>
			<dataPath>ДоступноДляПродажи</dataPath>
			<expression>Сумма(ДоступноДляПродажи)</expression>
		</totalField>
		<totalField>
			<dataPath>Обособленно</dataPath>
			<expression>Сумма(Обособленно)</expression>
		</totalField>
		<parameter>
			<name>Период</name>
			<title xmlns:d4p1="http://v8.1c.ru/8.1/data/core" xsi:type="d4p1:LocalStringType">
				<d4p1:item>
					<d4p1:lang>ru</d4p1:lang>
					<d4p1:content>Период</d4p1:content>
				</d4p1:item>
			</title>
			<valueType>
				<Type xmlns="http://v8.1c.ru/8.1/data/core">xs:dateTime</Type>
